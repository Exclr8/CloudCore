@inherits CloudCore.Web.Core.BaseViews.WebView<CloudCore.Admin.Models.WorkListItemModel>

@{
    ViewBag.Title = "Change Worklist Item Due Date";
}

@using (Html.BeginForm())
{
    using (FormLayout.BeginTable())
    {
        @FormLayout.AddDateTimePickerRowFor(m => m.ActivationSchedule);
    }
    FormLayout.AddSubmitButtonWithBar("Save");    
}