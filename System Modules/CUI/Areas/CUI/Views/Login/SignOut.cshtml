@{
    ViewBag.Title = "Sign Out";
}

<form class="form-signin">
    <div class="form-group">
        <h3>Successfully Signed Out</h3>
        <p />
        <label>You must close your browser to complete the sign out process.</label>
    </div>

    <div class="form-group">
        <button type="button" class="btn btn-default" onclick="location.href='@Url.Action("Index", "Login")'">Go back to site</button>
    </div>
</form>

