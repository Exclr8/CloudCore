    xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
    <TemplateData>
        <Name>CloudCore Process Module</Name>
        <Description>Template for CloudCore Process Module</Description>
        <Icon>processmodule.ico</Icon>
        <ProjectType>CSharp</ProjectType>
        <ProjectSubType>Eisk</ProjectSubType>
        <DefaultName>CloudCoreProcessModule</DefaultName>
        <TemplateGroupID>CloudCore</TemplateGroupID>
        <TemplateID>CloudCore.Module.Process</TemplateID>
      <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
